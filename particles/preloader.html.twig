    	animation: food 2s linear 2s infinite;
    }

    #bread2 {
    	animation: food 2s linear 1.5s infinite;
    }

    #bread3 {
    	animation: food 2s linear 1s infinite;
    }

    #bread4 {
    	animation: food 2s linear .5s infinite;
    }

    @keyframes food {
    	0% {
    		left: 200px;
    	}
    	100% {
    		left: 0px;
    	}
    }

    @keyframes pakuUp {
    	0% {
    		transform: rotate(-40deg);
    	}
    	50% {
    		transform: rotate(0deg);
    	}
    	100% {
    		transform: rotate(-40deg);
    	}
    }

    @keyframes pakuDown {
    	0% {
    		transform: rotate(40deg);
    	}
    	50% {
    		transform: rotate(0deg);
    	}
    	100% {
    		transform: rotate(40deg);
    	}
    }
    </style>

